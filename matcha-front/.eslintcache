e/Profile.tsx",["54"],"import React, { useState, useEffect } from 'react'\nimport './Profile.css'\nimport { Header, Image, Card, Icon, Button } from 'semantic-ui-react'\nimport { Gender, IUser } from '../../app/models/user'\n\ninterface IProps {\n\tuser: IUser\n\tgetProfile(): void; \n}\n\nconst Profile: React.FC<IProps> = ({ getProfile, user }) => {\n\tconsole.log(user)\n\n\tconst [location, setLocation] = useState({ lat: 0, lon: 0 })\n\n\tconst birth = new Date(user.birthday).getFullYear()\n\tconst nyt = new Date().getFullYear()\n\n\tconst getLocation = () => {\n\t\tnavigator.geolocation.getCurrentPosition((position) => {\n\t\t\tconsole.log(position.coords.latitude)\n\t\t\tconsole.log(position.coords.longitude)\n\t\t\tsetLocation({\n\t\t\t\tlat: position.coords.latitude,\n\t\t\t\tlon: position.coords.longitude,\n\t\t\t})\n\t\t})\n\t}\n\n\tuseEffect(() => {\n\t\tgetLocation()\n\t}, [])\n\n\tconst getDistance = (latA: number, latB: number, lonA: number, lonB: number) => {\n\t\tlet dLat = ((latA - latB) * Math.PI) / 180.0\n\t\tlet dLon = ((lonA - lonB) * Math.PI) / 180.0\n\n\t\tlatA = (latA * Math.PI) / 180.0\n\t\tlatB = (latB * Math.PI) / 180.0\n\n\t\tlet a =\n\t\t\tMath.pow(Math.sin(dLat / 2), 2) +\n\t\t\tMath.pow(Math.sin(dLon / 2), 2) * Math.cos(latA) * Math.cos(latB)\n\n\t\tlet rad = 6371\n\t\tlet c = Math.asin(Math.sqrt(a))\n\n\t\treturn rad * c\n\t}\n\n\treturn (\n\t\t<div>\n\t\t\t<Card fluid>\n\t\t\t\t<Image src={`https://robohash.org/${user.firstName}1`} wrapped ui={false} />\n\t\t\t\t<Image src={`https://robohash.org/${user.firstName}2`} wrapped ui={false} />\n\t\t\t\t<Image src={`https://robohash.org/${user.firstName}3`} wrapped ui={false} />\n\t\t\t\t<div className=\"profileinfo\">\n\t\t\t\t\t<Header as=\"h1\">\n\t\t\t\t\t\t{user.firstName} {user.lastName}\n\t\t\t\t\t</Header>\n\t\t\t\t\t<Icon name={user.gender === 'male' ? 'mars' : 'venus'} />\n\t\t\t\t\t<Card.Meta>\n\t\t\t\t\t\tDistance:{' '}\n\t\t\t\t\t\t{getDistance(\n\t\t\t\t\t\t\tlocation.lat,\n\t\t\t\t\t\t\tuser.location.latitude,\n\t\t\t\t\t\t\tlocation.lon,\n\t\t\t\t\t\t\tuser.location.longitude\n\t\t\t\t\t\t)}{' '}\n\t\t\t\t\t\tkm\n\t\t\t\t\t</Card.Meta>\n\t\t\t\t\tAge: {nyt - birth}\n\t\t\t\t\t<Card.Description>{user.biography}</Card.Description>\n\t\t\t\t\t<br></br>\n\t\t\t\t\t<Button\n\t\t\t\t\t\tcircular\n\t\t\t\t\t\ticon=\"cancel\"\n\t\t\t\t\t\tsize=\"massive\"\n\t\t\t\t\t\tcolor=\"black\"\n\t\t\t\t\t/>\n\t\t\t\t\t<Button\n\t\t\t\t\t\tcircular\n\t\t\t\t\t\ticon=\"like\"\n\t\t\t\t\t\tfloated=\"right\"\n\t\t\t\t\t\tsize=\"massive\"\n\t\t\t\t\t\tcolor=\"red\"\n\t\t\t\t\t\tonClick={getProfile}\n\t\t\t\t\t/>\n\t\t\t\t</div>\n\t\t\t</Card>\n\t\t</div>\n\t)\n}\n\nexport default Profile\n","/Users/anttivornanen/react-stuff/matcha/matcha-front/src/app/models/user.ts",["55"],"/Users/anttivornanen/react-stuff/matcha/matcha-front/src/features/browse/Browse.tsx",[],"/Users/anttivornanen/react-stuff/matcha/matcha-front/src/features/nav/Footer.tsx",[],"/Users/anttivornanen/react-stuff/matcha/matcha-front/src/features/matches/Matches.tsx",[],"/Users/anttivornanen/react-stuff/matcha/matcha-front/src/features/user/Login.tsx",[],"/Users/anttivornanen/react-stuff/matcha/matcha-front/src/features/user/Register.tsx",[],{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":4,"column":10,"nodeType":"62","messageId":"63","endLine":4,"endColumn":16},{"ruleId":"60","severity":1,"message":"64","line":27,"column":6,"nodeType":"62","messageId":"63","endLine":27,"endColumn":22},"no-native-reassign",["65"],"no-negated-in-lhs",["66"],"@typescript-eslint/no-unused-vars","'Gender' is defined but never used.","Identifier","unusedVar","'SexualPreference' is defined but never used.","no-global-assign","no-unsafe-negation"]